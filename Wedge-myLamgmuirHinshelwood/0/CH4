FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      CH4;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0799;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.0799;
    }
    outlet
    {
        type            zeroGradient;
    }
    "(exteriorWall|walls)"
    {
        type            zeroGradient; 
    }
    center
    {
        type            symmetryPlane;
    }
    "frontAndBack_.*"
    {
        type            wedge;
    }   
}


